/* 模态框 */
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 999999;
  color: #fff;
}
.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: 0 105rpx;
  border-radius: 36rpx;
  transform: translate(0,-50%);
}
.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}
.modal-content {
  padding: 50rpx 32rpx;
  font-size: 28rpx;
}
.modal-content .p{
  margin-bottom: 15rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}
.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}
.btn-confirm {
  width: 100%;
  color: #353897;
  text-align: center;
}

.cancel{
  position: absolute;
  z-index: 55;
  top: 30rpx;
  right: 0;
  font-size: 28rpx;
  border: 1px solid rgba(255, 255, 255, 0.219);
  color: rgba(255, 255, 255, 0.384);
  padding: 10rpx 30rpx;
  border-radius: 300rpx 0 0 300rpx;
  border-right: 0;
}



