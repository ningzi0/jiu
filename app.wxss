/* 无订单状态 */
.nothing{
  position:fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 100%;
}
.nothing .nothingImg image{
  width: 100px;
  height: 100px;
  display: block;
  margin: 0 auto;
}
.nothing .text{
  font-size: 24rpx;
  color: #626463;
  padding: 15rpx 0;
  text-align: center;
}
.nothing navigator{
  background-color: #0128a9;
  color: #fff;
  text-align: center;
  padding: 20rpx 30rpx;
  width: 200rpx;
  margin: 50rpx auto 0;
  font-size: 28rpx;
  width: 45%;
  border-radius: 200rpx;
}
/* 广告位 */
.myadv image{
