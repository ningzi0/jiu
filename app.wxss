page{
  background-color: #eee;
  font-weight: 300;
}
.boxshadow{
  box-shadow: 0 2rpx 10rpx rgba(0,0,0,0.1);
}
.swiper{
  width: 100%;
  height: 540rpx;
}
.swiper image{
  width: 100%;
  height: 540rpx;
}
.add_shopCart{
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
  width: 60rpx;
  height: 60rpx;
  background-color: #fff;
  text-align: center;
  line-height: 56rpx;
  font-size: 40rpx;
  font-weight: bold;
  border-radius: 50%;
  color: #2030a7;
}



radio .wx-radio-input {
  border-radius: 50%;


  width: 24px;
  height: 24px;
}
radio .wx-radio-input {
  border-color: #999;
}
radio .wx-radio-input.wx-radio-input-checked {
  border-color: #0623ad !important;


  background: white !important;
}
radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;


  width: 20px;


  height: 20px;


  content: "";


  background-color: #0623ad;
}


.p_price_num_r_con{
  display: inline-block;
}
.item-center{
  display: flex;
  width: 100%;
}
.buy-num-btn{
  width: 60rpx;
  height: 60rpx;
  background: #0724ae;
  color: #fff;
  text-align: center;
  line-height: 54rpx;
  border-radius: 50%;
  font-size: 40rpx;
  display: inline-block;
  vertical-align: middle;
}
.btn-minus{
