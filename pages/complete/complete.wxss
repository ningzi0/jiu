/* pages/complete/complete.wxss */
.c_header{
  text-align: center;
  padding: 70rpx 0 0;
}
.c_header .p{
  font-size: 28rpx;
  color: #878787;
}
.c_aglin{
  padding: 20rpx 100rpx;
  border: 1px solid #062578;
  display: inline-block;
  border-radius: 100rpx;
  color: #062578;
  margin: 30rpx 0;
  background-color: #fff;
}
.adv{
  width: 95%;
  height: 123rpx;
  margin: 0 auto;
}
.adv navigator{
  width: 100%;
  height: 100%;
  display: block;
}
.adv image{
  width: 100%;
  height: 100%;
}

.c_add{
  width: 95%;
  margin: 15rpx auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
}
.c_add .c_h3{
  font-size: 34rpx;
  font-weight: bold;
}
.c_add .c_span{
  font-size: 28rpx;
  color: #a7a7a7;
  margin-top: 10rpx;
}


.c_orderInfo{
  width: 95%;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 20rpx;
  box-sizing: border-box;
}


.c_o_tit{
  font-size: 24rpx;
  color: #9fa3a6;
  border-left: 10rpx solid #062578;
  padding-left: 13rpx;
  margin-bottom: 15rpx;
}
.c_o_titName{
  border: 0;
  font-size: 30rpx;
}
.c_o_titName image{
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
}
.c_o_titName text{
  display: inline-block;
  vertical-align: middle;
}
.c_orderInfo .c_o_con .img image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.c_orderInfo .c_o_con .ul .li{
  margin: 20rpx 0;
}
.c_orderInfo .c_o_con .img{
  display: inline-block;
  vertical-align: middle;
}
.c_orderInfo .c_o_con .c_o_info{
  display: inline-block;
  vertical-align: middle;
  width: 410rpx;
  font-size: 30rpx;
  padding: 0 15rpx;
}
.c_orderInfo .c_o_con .c_o_info .h3{
  font-weight: bold;
}
.c_orderInfo .c_o_con .c_o_info .span{
  font-size: 28rpx;
  color: #878787;
  line-height: 1.75;
}
.c_orderInfo .c_o_con .c_o_num{
  display: inline-block;
  vertical-align: middle;
  text-align: right;
  line-height: 1.5;
}
.c_orderInfo .c_o_con .c_o_num .c_o_price{
  font-size: 30rpx;
  font-weight: bold;
}
.c_orderInfo .c_o_con .c_o_num .c_o_num{
  font-size: 28rpx;
  color: #878787;
}

.priceInfoItem{
  border-bottom: 1px dashed #eaeaea;
  padding: 10rpx 0;
}
.priceInfoItem:last-child{
  border: 0;
}
.priceInfoItemLi{
  display: flex;
  padding: 15rpx 0;
  font-size: 28rpx;
}
.priceInfoItemLi text{
  flex: 1;
}
.priceInfoprice{
  text-align: right;
}
.completePrice{
  font-weight: bold;
  font-size: 34rpx;
}
.time{
  background-color: #fff;
  width: 95%;
  margin: 15rpx auto;
  padding: 0 20rpx;
  box-sizing: border-box;
}



/* 联系商家  联系骑手 */
.c_header .btn{
  display: flex;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
}
.c_header .btn button{
  width: 100%;
}




/* 订单核销码 */
.erweima image{
    width: 300rpx;
    height: 300rpx;
}






.logistics{
  background: #fff;
  margin: 20rpx;
  font-size: 26rpx;
  padding: 20rpx;
  border-radius: 10rpx;
}
.logisticsName{
  padding: 15rpx 0;
  display: flex;
  align-items: center;
}
.logisticsName image{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  margin-right: 15rpx;
}
.logisticsCon .li{
  padding: 15rpx 0;
}
.logisticsCon .li.active .li_title{
  color: rgb(255, 60, 0);
}
.logisticsCon .li.active .li-con{
  color: rgb(2, 2, 2);
}
.li_title{
  display: flex;
  align-items: flex-end;
  padding: 15rpx 0;
  color: rgb(146, 146, 146);
}
.li-con{
  color: rgb(146, 146, 146);
}
.li-title-font{
  font-size: 34rpx;
  font-weight: bold;
  padding-right: 10rpx;
}
.li-title-date{
  flex: 3;
}





/* 物流弹窗 */
.arrow{
  display:inline-block;
  border:6px solid transparent;
  border-top-color:#000;
  margin-left:8px;
  position:relative;
  top:6rpx;
 }
 /* ---------------------------- */
 /*模态框*/
 .modal{position:fixed; top:0; right:0; bottom:0; left:0; z-index:1000;}
 .modal-cancel{position:absolute; z-index:2000; top:0; right:0; bottom: 0; left:0; background:rgba(0,0,0,0.3);}
 .bottom-dialog-body{width:100%; position:absolute; z-index:3000; bottom:0; left:0;background:#fff;}
 /*动画前初始位置*/
 .bottom-positon{-webkit-transform:translateY(100%);transform:translateY(100%);}
   
   
 /* 底部弹出框 */


 .li-con navigator{
   display: inline;
   color: rgb(255, 60, 0);
 }
 .Mcancel{
  background: #fff;
  padding: 26rpx 0;
  text-align: center;
  border-top: 1px solid #eee;
 }
 .li-con{
   padding: 10rpx 0;
 }
 .Mselect .title{
   font-size: 34rpx;
   font-weight: bold;
   text-align: center;
   padding: 30rpx 0;
   background: #eee;
 }
 .Mselect .logistics {
   max-height: 1000rpx;
   min-height: 600rpx;
   overflow-y: scroll;
 }