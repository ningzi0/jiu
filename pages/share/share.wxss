/* pages/share/share.wxss */
page{
  background-color: #fff;
}
.erweima{
  width: 500rpx;
  height: 500rpx;
  margin: 100rpx auto 0;
  border: 1px solid #eee;
  padding: 30rpx;
  border-radius: 30rpx;
  box-sizing: border-box;
  box-shadow: 0 0 30rpx rgba(0,0,0,0.1);
}
.erweima image{
  width: 100%;
  height: 100%;
}
.share button{
  background-color: #ffe726;
  width: 500rpx !important;
  margin: 70rpx auto;
  font-weight: 500;
  font-size: 29rpx;
  border-radius: 200rpx;
  padding: 30rpx 0;
  color: #000;
}
.share button:active{
  background: #d6bf0c;
}
.content{
  width: 600rpx;
  margin: 0 auto;
  font-size: 29rpx;
  line-height: 1.75;
}


.Commission_details{
  display: block;
  text-align: center;
  margin: 30rpx;
  padding: 30rpx 0;
}







