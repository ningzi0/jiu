/* pages/feedback/feedback.wxss */
.feedbackWrap{
  background-color: #fff;
  margin: 15rpx;
  border-radius: 8rpx;
  padding: 20rpx;
}
.feedbackWrap .ul .li textarea{
  padding: 20rpx;
  border: 1px solid #eee;
  font-size: 28rpx;
  width: 100%;
  box-sizing: border-box;
}
.feedbackWrap button.submit{
  background:linear-gradient(to bottom right ,#fde4bb , #febe74);
  width: 100%;
  margin-top: 20rpx;
}


