/* pages/integralDel/integralDel.wxss */
.integraCard{
  display: flex;
  align-items: center;
  margin: 20rpx;
  background-color: #fff;
  padding: 40rpx 40rpx;
  border-radius: 20rpx;
}
.integraC_left{
  flex: 2;
  font-size: 29rpx;
  color: #666;
}
.integraC_left .Number{
  font-size: 65rpx;
  font-family: Arial, Helvetica, sans-serif;
  font-weight: bold;
  margin: 15rpx 0;
  color: #000;
}
.integraC_right{
  flex: 1;
  text-align: right;
}
.integraC_right navigator{
  display: inline-block;
  background-color: #ffe726;
  padding: 10rpx 30rpx;
  border-radius: 200rpx;
  font-size: 29rpx;
}
.integraContent_c{
  font-size: 29rpx;
  color: #666;
  padding: 20rpx;
}
.integraContent_c .item{
  background-color: #fff;
  margin: 20rpx 0;
  padding: 30rpx;
  border-radius: 20rpx;
  line-height: 2;
}
.integraContent_c .item .date{
  font-size: 32rpx;
  font-weight: bold;
  color: #000;
  position: relative;
  padding-left: 30rpx;
}
.integraContent_c .item .date::before{
  content: "";
  left: 0;
  display: block;
  position: absolute;
  height: 70%;
  width: 10rpx;
  background-color: #ffe726;
  top: 50%;
  transform: translate(0,-50%);
  border-radius: 5rpx;
}
.integraContent_c .item .jifen{
  display: flex;
  font-size: 40rpx;
  font-weight: bold;
  color: #000;
}
.integraContent_c .item .jifen text{
  flex: 1;
}
.integraContent_c .item .jifen text:last-child{
  text-align: right;
}
.integraContent_c .item .time{
  color: #999;
}
.integraContent_c .item .time text{
  color: #000;
}
